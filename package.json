{
  "name": "hands-on-k6-pg-iceberg",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "author": "Damir Manapov",
  "license": "MIT",
  "scripts": {
    "dev": "tsx src/server.ts",
    "dev:cluster": "tsx scripts/cluster.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "lint": "eslint .",
    "format": "prettier -w \"**/*.{ts,js,md,json,yml,yaml}\"",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "test:e2e": "vitest run -c vitest.e2e.config.ts",
    "k6": "k6 run",
    "stats": "tsx scripts/stats.ts",
    "optimize": "tsx scripts/optimize.ts",
    "compose:up": "docker compose -f compose/docker-compose.yml --profile single up starter",
    "compose:up:cluster": "docker compose -f compose/docker-compose.yml --profile cluster up starter-cluster",
    "compose:down": "COMPOSE_PROFILES=single,cluster docker compose -f compose/docker-compose.yml down",
    "compose:restart": "COMPOSE_PROFILES=single,cluster docker compose -f compose/docker-compose.yml restart",
    "compose:reset": "COMPOSE_PROFILES=single,cluster docker compose -f compose/docker-compose.yml down --volumes --remove-orphans"
  },
  "dependencies": {
    "express": "^5.2.1",
    "kysely": "^0.28.9",
    "pg": "^8.16.3",
    "pg-format": "^1.0.4",
    "trino-client": "^0.2.9"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "@types/pg-format": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
